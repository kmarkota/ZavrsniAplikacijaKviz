<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/big_vertical"
    tools:context=".ResultActivity">

    <TextView
        android:id="@+id/tekst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="99dp"
        android:layout_marginTop="71dp"
        android:text="Hvala na strpljenju!"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/link"
        android:layout_width="297dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"

        android:layout_alignParentStart="true"
        android:layout_marginBottom="188dp"
        android:autoLink="all"
        android:clickable="true"
        android:text="https://www.bizit.hr/sigurnost-mobilnih-uredaja-mane-i-prednosti-komuniciranja-u-pokretu/"
        android:textSize="20dp"
        tools:text="Mane i prednosti komuniciranja u pokretu" />

    <TextView
        android:id="@+id/link2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:autoLink="all"
        android:clickable="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="68dp"
        android:layout_marginTop="256dp"
        android:text="TextView"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/link3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/link"
        android:autoLink="all"
        android:clickable="true"
        android:layout_marginTop="220dp"
        android:text="TextView"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tekst2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90dp"
        android:layout_marginTop="130dp"
        android:text="TextView" />

</RelativeLayout>